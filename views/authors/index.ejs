<h2>All Authors</h2>
<form action="/authors" method="GET">
  <label for="">Search Authors</label>
  <input type="text" name="name" value="<%= searchOptions.name %>">
  <button type="submit">Submit</button>
</form>
<br>
<% authors.forEach(author => { %>
      <div><%= author.name %> </div>
      <% }) %> 
